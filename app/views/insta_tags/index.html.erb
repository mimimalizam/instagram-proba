<p id="notice"><%= notice %></p>

<h1>Listing Insta Tags</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @insta_tags.each do |insta_tag| %>
      <tr>
        <td><%= insta_tag.name %></td>
        <td><%= link_to 'Show', insta_tag %></td>
        <td><%= link_to 'Edit', edit_insta_tag_path(insta_tag) %></td>
        <td><%= link_to 'Destroy', insta_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Insta tag', new_insta_tag_path %>
